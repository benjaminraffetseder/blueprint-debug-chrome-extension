{
    "name": "Blueprint",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Debug your CSS",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "script.js"
        ]
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "file:///*"
    ]
}